	<queryString>
		<![CDATA[SELECT PRE.TRANSACTION_DATE, PRE.DOC_REF, UPPER(GCM.COMPANY_NAME) AS COMPANY_NAME, SLM.LINE_NAME, SVH.VOYAGE_NO,
SVM.VESSEL_NAME || ' / '|| SVH.VOYAGE_NO AS VESSEL_NAME, SPM.PORT_NAME, PEH.DOC_REF AS PDA_REF
FROM PDA_RORO_ENTRY_HDR PRE
INNER JOIN SHIP_VOYAGE_HDR SVH ON SVH.TRANSACTION_POID = PRE.VESSEL_VOYAGE_POID
INNER JOIN SHIP_VESSEL_MASTER SVM ON SVM.VESSEL_POID = SVH.VESSEL_POID
INNER JOIN SHIP_LINE_MASTER SLM ON SLM.LINE_POID = SVH.LINE_POID
LEFT JOIN SHIP_PORT_MASTER SPM ON SPM.PORT_POID = SVH.DESTINATION_PORT_POID
INNER JOIN GLOBAL_COMPANY_MASTER GCM ON GCM.COMPANY_POID = PRE.COMPANY_POID
LEFT JOIN PDA_ENTRY_HDR PEH ON PEH.VOYAGE_POID = SVH.TRANSACTION_POID AND PEH.VOYAGE_NO = SVH.VOYAGE_NO
WHERE PRE.TRANSACTION_POID = $P{DOC_KEY_POID}]]>
	</queryString>
