    <queryString>
        <![CDATA[SELECT PEH.DOC_REF AS PDA_REF,PFH.DOC_REF,
CASE WHEN PFH1.DOC_REF IS NOT NULL THEN 'Supplementary FDA of '||PFH1.DOC_REF
     END SUPPLEMENTARY_FDA_REF,
NVL(PFH.GL_CLOSING_DATE,PFH.TRANSACTION_DATE) AS TRANSACTION_DATE,PFH.GROUP_POID,PFH.COMPANY_POID,
NVL(SPM.PRINCIPAL_NAME2, SPM.PRINCIPAL_NAME) AS PRINCIPAL_NAME,PFH.PRINCIPAL_CONTACT,PFH.DOC_REF,
SVM.VESSEL_NAME,PFH.ARRIVAL_DATE,PFH.SAIL_DATE,SPM.PORT_NAME,PFH.OPERATION_TYPE,PFH.COMODITY_POID,
PFH.UNIT,PFH.HARBOUR_CALL_TYPE,PFH.CURRENCY_CODE,PFH.CURRENCY_RATE,PFH.VOYAGE_NO,
PFH.REMARKS,PFH.TOTAL_AMOUNT,PFH.IMO_NUMBER,
CASE WHEN PFH.GRT IS NOT NULL THEN PFH.GRT ||' MT'
     ELSE TO_CHAR(PFH.GRT) END AS GRT,
CASE WHEN PFH.NRT IS NOT NULL THEN PFH.NRT ||' MT'
     ELSE TO_CHAR(PFH.NRT) END AS NRT,
CASE WHEN PFH.DWT IS NOT NULL THEN PFH.DWT ||' MT'
     ELSE TO_CHAR(PFH.DWT) END AS DWT,
CASE WHEN PFH.TOTAL_QUANTITY IS NOT NULL THEN PFH.TOTAL_QUANTITY ||'  '||SM.STOCK_UNIT_CODE ELSE TO_CHAR(PFH.TOTAL_QUANTITY) END AS TOTAL_QUANTITY,
FN_AMOUNT_IN_WORDS(GET_FDA_SUM(PFH.TRANSACTION_POID)) FDASUM,
GCM.ADDRESS, GCM.COMPANY_NAME, GCM.ACCOUNT_PERSON, GCM.EMAIL, GBM.BANK_ADDRESS,GBM.SWIFT_CODE,GBM.IBAN,GBM.BANK_ACCOUNT_NO,GBM.REMARKS AS BANK_REMARKS,
NVL(T.REMITTANCE_AMOUNT,0) AS REMITTANCE_AMOUNT,PFH.PORT_CALL_NUMBER
FROM PDA_FDA_HDR PFH
LEFT JOIN PDA_ENTRY_HDR PEH ON PEH.TRANSACTION_POID=PFH.PDA_REF
LEFT JOIN SHIP_PRINCIPAL_MASTER SPM ON SPM.PRINCIPAL_POID=PFH.PRINCIPAL_POID
LEFT JOIN SHIP_VESSEL_MASTER SVM ON SVM.VESSEL_POID=PFH.VESSEL_POID
LEFT JOIN SHIP_PORT_MASTER SPM ON SPM.PORT_POID=PFH.PORT_POID
LEFT JOIN STOCK_UNIT_MASTER SM ON SM.STOCK_UNIT_CODE=PFH.UNIT
LEFT JOIN GLOBAL_COMPANY_MASTER GCM ON PEH.COMPANY_POID=GCM.COMPANY_POID
LEFT JOIN GL_BANK_MASTER GBM ON GBM.BANK_POID=NVL(PFH.PRINT_BANK_POID,GCM.BANK_POID)
LEFT JOIN
        (SELECT GRA.ADVANCE_REF_POID AS REF_NUMBER,SUM(NVL(GRA.AMOUNT,0)) AS REMITTANCE_AMOUNT
        FROM AR_GEN_RECEIPT_ADVANCE_DTL GRA
        INNER JOIN PDA_FDA_HDR PFH ON PFH.TRANSACTION_POID=GRA.ADVANCE_REF_POID
        WHERE GRA.ADVANCE_REF_DOC_ID='110-161'
        GROUP BY GRA.ADVANCE_REF_POID
        )T  ON T.REF_NUMBER=PFH.TRANSACTION_POID
LEFT JOIN PDA_FDA_HDR PFH1 ON PFH1.TRANSACTION_POID=PFH.SUPPLEMENTARY_FDA_POID
WHERE PFH.TRANSACTION_POID=$P{DOC_KEY_POID}]]>
    </queryString>
