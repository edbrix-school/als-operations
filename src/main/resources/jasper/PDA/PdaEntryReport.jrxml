	<queryString>
		<![CDATA[SELECT RTN_GLOBAL_PARAMETER( 1,'GLOBAL_TAX_APPLICABLE','TAX',PEH.COMPANY_POID,NULL) AS TAX_APPLICABLE,SPM.TIN_NUMBER,GCM.TIN_NUMBER AS COMPANY_TIN_NUMBER,
PEH.TRANSACTION_POID, PEH.TRANSACTION_DATE, SPM.PRINCIPAL_NAME, PEH.PRINCIPAL_CONTACT,
PEH.VOYAGE_NO,SVM.VESSEL_NAME,PEH.ARRIVAL_DATE,PEH.SAIL_DATE,SPM.PORT_NAME,PEH.COMODITY_POID
, PEH.OPERATION_TYPE, PEH.IMPORT_QTY ||'  '||SM.STOCK_UNIT_NAME AS IMPORT_QTY, PEH.EXPORT_QTY ||'  '||SM.STOCK_UNIT_NAME AS EXPORT_QTY,
PEH.TOTAL_QUANTITY ||'  '||SM.STOCK_UNIT_NAME AS TOTAL_QUANTITY, PEH.UNIT,
PEH.HARBOUR_CALL_TYPE, PEH.CURRENCY_CODE, PEH.CURRENCY_RATE,SM.STOCK_UNIT_NAME,
PEH.VESSEL_VERIFIED, PEH.VESSEL_VERIFIED_DATE, PEH.VESSEL_VERIFIED_BY, PEH.URGENT_APPROVAL,
PEH.PRINCIPAL_APRVL_DAYS, PEH.PRINCIPAL_APPROVED, PEH.PRINCIPAL_APPROVED_DATE,
PEH.PRINCIPAL_APPROVED_BY, PEH.REMINDER_MINUTES, PEH.REMARKS, PEH.TOTAL_AMOUNT, PEH.CREATED_BY,
PEH.CREATED_DATE, PEH.LASTMODIFIED_BY, PEH.LASTMODIFIED_DATE, PEH.DELETED, PEH.CARGO_DETAILS,
PEH.STATUS, PEH.DOC_REF, PEH.ADDRESS_POID, PEH.SALESMAN_POID, PEH.TRANSHIPMENT_QTY ||' '||PEH.UNIT AS TRANSHIPMENT_QTY,
PEH.VESSEL_TYPE_POID,  PEH.GRT ||'  MT' AS GRT, PEH.NRT ||'  MT' AS NRT, PEH.DWT ||'  MT' AS DWT,PEH.TERMS_POID,PEH.PORT_DESCRIPTION,
GCM.ADDRESS,GCM.COMPANY_NAME,GBM.BANK_ADDRESS,GBM.SWIFT_CODE,GBM.IBAN,GBM.BANK_ACCOUNT_NO,GBM.REMARKS AS BANK_REMARKS
FROM PDA_ENTRY_HDR PEH
LEFT JOIN SHIP_PRINCIPAL_MASTER SPM ON SPM.PRINCIPAL_POID=PEH.PRINCIPAL_POID
LEFT JOIN SHIP_VOYAGE_HDR SVH ON SVH.TRANSACTION_POID=PEH.VOYAGE_POID
LEFT JOIN SHIP_VESSEL_MASTER SVM ON SVM.VESSEL_POID=PEH.VESSEL_POID
LEFT JOIN SHIP_PORT_MASTER SPM ON SPM.PORT_POID=PEH.PORT_POID
LEFT JOIN STOCK_UNIT_MASTER SM ON SM.STOCK_UNIT_CODE=PEH.UNIT
LEFT JOIN GLOBAL_COMPANY_MASTER GCM ON PEH.COMPANY_POID=GCM.COMPANY_POID
LEFT JOIN GL_BANK_MASTER GBM ON GBM.BANK_POID=GCM.BANK_POID
WHERE PEH.TRANSACTION_POID=$P{DOC_KEY_POID}]]>
	</queryString>
